{
	"name": "types-ddd",
	"version": "2.8.8",
	"description": "This package provide utils file and interfaces to assistant build a complex application with domain driving design",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"license": "ISC",
	"author": "Alessandro Dev",
	"engines": {
		"node": ">=14"
	},
	"keywords": [
		"Complexity",
		"NodeJS",
		"Business Logic",
		"DDD",
		"Domain Driving Design",
		"Typescript",
		"DDD-Utils",
		"Base Entity",
		"Base Aggregate",
		"Base Value Object",
		"Use Cases",
		"Domain Events",
		"Clean Architecture"
	],
	"scripts": {
		"prebuild": "rimraf ./dist && yarn check:circular-deps",
		"build": "tsc -p tsconfig.lib.json",
		"test": "jest --silent --runInBand",
		"test:verbose": "jest --runInBand",
		"check:circular-deps": "madge --circular --extensions ts ./lib",
		"prepublish:lib": "rimraf ./dist && yarn check:circular-deps",
		"publish:lib": "npm publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/4lessandrodev/types-ddd.git"
	},
	"bugs": {
		"url": "https://github.com/4lessandrodev/types-ddd/issues"
	},
	"homepage": "https://alessandroadm.gitbook.io/types-ddd/",
	"dependencies": {
		"bcrypt": "5.0.1",
		"pino": "6.13.4",
		"pino-pretty": "7.3.0"
	},
	"devDependencies": {
		"@microsoft/tsdoc": "0.13.2",
		"@types/bcrypt": "5.0.0",
		"@types/jest": "27.0.1",
		"@types/node": "16.9.1",
		"@types/pino": "6.3.11",
		"jest": "27.1.1",
		"madge": "^5.0.1",
		"rimraf": "3.0.2",
		"ts-jest": "27.0.5",
		"ts-node": "10.2.1",
		"typescript": "4.4.2"
	},
	"files": [
		"dist/*",
		"package.json"
	]
}